<h1>Listing studies</h1>
<%= link_to 'New study', new_study_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
  </tr>
 
  <% @studies.each do |study| %>
    <tr>
      <td><%= study.title %></td>
      <td><%= study.text %></td>
      <td><%= link_to 'Show', study_path(study) %></td>
      <td><%= link_to 'Edit', edit_study_path(study) %></td>
    </tr>
  <% end %>
</table>